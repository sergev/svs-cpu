*
* Test for instruction AVX.
*
start   start   '1'
        vtm     stack(15)
        ntr     3                       , без нормализации и округления
        vtm     64(14)
        ita     14                      , целое 64 без порядка
        avx     plus1
        u1a     fail                    , положительное
        aex     =f'64'
        u1a     fail
*
        ita     14                      , целое 64 без порядка
        avx     minus1
        uza     fail                    , отрицательное
        aex     =b'0037777777777700'
        u1a     fail
*-------------------------
        xta     minus1
        avx     minus1
        u1a     fail                    , положительное
        aex     plus1
        u1a     fail
*-------------------------
        xta     plus1
        avx     minus1
        uza     fail                    , отрицательное
        aex     =b'4070000000000000'    , ненормализованная -1
        u1a     fail
*-------------------------
        xta     =e'-2'
        xts     =e'-3'
        avx     (15)                    , 3
        u1a     fail
*
        aex     =e'3'
        u1a     fail
*-------------------------
        ntr     0                       , с нормализацией и округлением
        xta     minus1
        avx     minus1
        u1a     fail                    , положительное
        aex     plus1                   , с нормализацией будет 1
        u1a     fail
*-------------------------
        xta     plus1
        avx     minus1
        uza     fail                    , отрицательное
        aex     minus1                  , нормализованная -1
        u1a     fail
*-------------------------
        xta     =b'7757777777777777'    , максимальное положительное
        avx     minus1
        uza     fail                    , отрицательное
        aex     =b'7760000000000001'
        u1a     fail
*-------------------------
        xta     =b'7760000000000000'    , максимальное отрицательное
        avx     minus1
        u1a     fail                    , положительное
        aex     =b'0010000000000000'    , переполнение
        u1a     fail
*-------------------------
        xta     =b'0010000000000000'    , минимальное положительное
        avx     minus1
        u1a     fail                    , положительное
        aex                             , опустошение
        u1a     fail
*-------------------------
        xta     =b'0027777777777777'    , минимальное отрицательное
        avx     minus1
        u1a     fail                    , положительное
        aex     =b'0010000000000001'
        u1a     fail
*-------------------------
pass    стоп    '12345'(6)
fail    стоп    '76543'(2)
*-------------------------
align   адрес   start+'1777'            , данные с адреса 2000
plus1   конд    e'1'                    , 1.0
minus1  конд    e'-1'                   , -1.0
stack   пам     10                      , стек
        fin
